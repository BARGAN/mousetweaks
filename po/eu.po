msgid "_Preferences"
msgstr "_Hobespenak"

#: ../data/mousetweaks.schemas.in.h:1
msgid "Button style"
msgstr "Botoi-estiloa"

#: ../data/mousetweaks.schemas.in.h:2
msgid "Button style in click type window (0=Text, 1=Icon, 2=Both)"
msgstr "Botoi-estiloa klik motaren leihoan (0=testua, 1=ikonoa, 2=biak)"

#: ../data/mousetweaks.schemas.in.h:3
msgid "Direction to perform a double click (0=Left, 1=Right, 2=Up, 3=Down)"
msgstr ""
"Norabidea klik bikoitza lantzeko (0=ezkerrera, 1=eskuinera, 2=gora, 3=behera)"

#: ../data/mousetweaks.schemas.in.h:4
msgid "Direction to perform a drag click (0=Left, 1=Right, 2=Up, 3=Down)"
msgstr ""
"Norabidea arrastatzeko klik-a lantzeko (0=ezkerrera, 1=eskuinera, 2=gora, "
"3=behera)"

#: ../data/mousetweaks.schemas.in.h:5
msgid "Direction to perform a secondary click (0=Left, 1=Right, 2=Up, 3=Down)"
msgstr ""
"Norabidea 2. mailako klik-a lantzeko (0=ezkerrera, 1=eskuinera, 2=gora, "
"3=behera)"

#: ../data/mousetweaks.schemas.in.h:6
msgid "Direction to perform a single click (0=Left, 1=Right, 2=Up, 3=Down)"
msgstr ""
"Norabidea klik bakuna lantzeko (0=ezkerrera, 1=eskuinera, 2=gora, 3=behera)"

#: ../data/mousetweaks.schemas.in.h:7
msgid "Distance in pixels before movement will be recognized"
msgstr "Distantzia pixeletan mugimendua antzeman aurretik"

#: ../data/mousetweaks.schemas.in.h:8
msgid "Dwell click mode"
msgstr "Klik-tenporizatuaren modua"

#: ../data/mousetweaks.schemas.in.h:9
msgid "Dwell click mode (0=Gesture mode, 1=Window mode)"
msgstr "Klik-tenporizatuaren modua (0=keinu-modua, 1=leiho-modua)"

#: ../data/mousetweaks.schemas.in.h:10
msgid "Dwell click time"
msgstr "Klik-tenporizatuaren denbora"

#: ../data/mousetweaks.schemas.in.h:11
msgid "Enable dwell click"
msgstr "Gaitu klik-tenporizatua"

#: ../data/mousetweaks.schemas.in.h:12
msgid "Enable secondary click"
msgstr "Gaitu 2. mailako klik-a"

#: ../data/mousetweaks.schemas.in.h:13
msgid "Enable simulated secondary click"
msgstr "Gaitu simulatutako 2. mailako klik-a"

#: ../data/mousetweaks.schemas.in.h:14
msgid "Gesture double click"
msgstr "Klik bikoitzaren keinua"

#: ../data/mousetweaks.schemas.in.h:15
msgid "Gesture drag click"
msgstr "Arrastatzeko klik-aren keinua"

#: ../data/mousetweaks.schemas.in.h:16
msgid "Gesture secondary click"
msgstr "2. mailako klik-aren keinua"

#: ../data/mousetweaks.schemas.in.h:17
msgid "Gesture single click"
msgstr "Klik bakunaren keinua"

#: ../data/mousetweaks.schemas.in.h:18
msgid "Movement threshold"
msgstr "Mugimenduaren atalasea"

#: ../data/mousetweaks.schemas.in.h:19
msgid "Secondary click time"
msgstr "2. mailako klik-aren denbora"

#: ../data/mousetweaks.schemas.in.h:20 ../src/mt-main.c:432
msgid "Show click type window"
msgstr "Erakutsi klik motaren leihoa"

#: ../data/mousetweaks.schemas.in.h:21
msgid "Time in seconds before a click is triggered"
msgstr "Denbora segundotan klik bat abiarazi aurretik"

#: ../data/mousetweaks.schemas.in.h:22
msgid "Time in seconds before a secondary click is triggered"
msgstr "Denbora segundotan 2. mailako klik bat abiarazi aurretik"

